{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "indent": [
            true,
            "spaces",
            4
        ],
        "max-line-length": [
            true,
            120
        ],
        "no-console": [
            true,
            "log",
            "error"
        ],
        "prefer-const": [
            false,
            {
                "destructuring": "all"
            }
        ]
    }
}